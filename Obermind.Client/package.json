{
  "name": "obermind_purchase_order_portal",
  "version": "1.0.0",
  "description": "React Redux / ASP.Net Core based Obermind Purchase Order Application",
  "scripts": {
    "start": "webpack-cli serve --mode=development --env development --open --hot --config ./webpack/webpack.config.dev.js",
    "clean": "rimraf node_modules && rimraf dist && npm install",
    "webpack:dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack.config.dev.js",
    "webpack:prod": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.prod.js",
    "build": "rimraf dist && npm run webpack:dev && npm run start",
    "build:prod": "rimraf dist && npm run webpack:prod && npm run start"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "Purchase Order",
    "Obermind",
    "react",
    "redux"
  ],
  "author": "Taimoor Adil Badsshah <taimurad@hotmail.com>",
  "license": "MIT",
